public class Commands
{
  public static void clear(int l)
  {
    for (int i = 0; i < 30 - l; i++)
    {
      System.out.println(" ");
    }
  }
  public void basic(String s)
  {
    if (s == "help")
    {
      System.out.println("Type LOOK to examine your surroundings. Type LOOK -item- to examine a specific item.");
      System.out.println("Type USE -item- to use an item that you have aquired. Type GET -item- to attempt to");
      System.out.println("get the item. Type TALK -person- to attempt conversation. Type GO -direction- to");
      System.out.println("relocate yourself.");
      clear(4);
    }
    else if (s == "look self")
    {
      System.out.println("You are a stout man of about 35. You are suffering from chronic undiagnosed amnesia.");
      clear(1);
    }
    public static void main(String args[])
    {
      Scanner kb = new Scanner(System.in);
      String s = kb.next();
      basic(s);
    }
